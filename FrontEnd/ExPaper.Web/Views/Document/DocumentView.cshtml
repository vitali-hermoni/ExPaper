@model DocumentDto

<!DOCTYPE html>
<html>
<head>
    <title>PDF Viewer</title>
    <meta name="viewport" content="width=device-width, initial-scale=1">

    <style>
        object, iframe {
            background-color: white;
        }
    </style>
</head>
<body>
    <div>
        <object data='@Model.Path' style="width: 100%; height: 678px;">
            <iframe src='@Model.Path' style="width: 100%; height: 678px;"></iframe>
        </object>
    </div>
</body>
</html>
